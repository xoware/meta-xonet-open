#rootfs               /                  auto       defaults              1  1
proc                 /proc              proc       defaults              0  0
sysfs                /sys               sysfs      defaults              0  0
devtmpfs             /dev/              devtmpfs   defaults              0  0
usbdevfs             /proc/bus/usb      usbdevfs   noauto                0  0
/dev/mmcblk0p3       /xokcfg            auto      defaults,noatime,rw   0  0
/dev/mmcblk0p4       /storage           auto      defaults,noatime,rw              0  0
tmpfs                /tmp               tmpfs      size=24M,mode=0777    0  0
tmpfs                /var/volatile      tmpfs      size=512K,mode=0777    0  0
tmpfs                /run	        tmpfs      size=512K,mode=0777    0  0
